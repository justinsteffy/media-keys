{
	"name": "Media Keys",
	"description": "Allows using previous, play/pause, and next keys for Grooveshark.",
	"version": "0.0.9",
	"manifest_version": 2,
	"content_scripts": [{
		"matches": ["*://grooveshark.com/*"],
		"js": ["grooveshark.js"]
	},
	{
		"matches": ["*://www.rhapsody.com/*"],
		"js": ["rhapsody.js"]
	}]
}
