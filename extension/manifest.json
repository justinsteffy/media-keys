{
	"name": "Media Keys",
	"description": "Allows using previous, play/pause, and next keys for Grooveshark, Google Play Music and Rhapsody.",
	"version": "0.0.10",
	"manifest_version": 2,
	"content_scripts": [{
		"matches": ["*://grooveshark.com/*"],
		"js": ["common.js", "grooveshark.js"]
	},
	{
		"matches": ["*://www.rhapsody.com/*"],
		"js": ["common.js", "rhapsody.js"]
	}, 
	{
		"matches": ["*://play.google.com/music/*"],
		"js": ["common.js", "gpm.js"]
	}]
}
